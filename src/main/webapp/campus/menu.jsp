<div class="container-fluid">
	<div class="row">
		<div class="panel panel-info">
			<div class="panel-heading">
				<span class="glyphicon glyphicon-home"> Campus</span>
			</div>
			<div class="panel-body">
				<form role="form">
				
					<div class="form-group">
						<label for="selectCampus">Seleccionar Campus:</label> <select
							class="form-control" id="selectCampus">
							<option>Campus 1</option>
							<option>Campus2</option>
						</select>
					</div>
					
					<div class="form-group">
						<label for="selectCoordinador">Coordinador del Campus:</label> <select
							class="form-control" id="selectCoordinador">
							<option>Coordinador 01</option>
							<option>Coordinador 02</option>
							
						</select>
					</div>
					<div class="form-group">
						<label for="txtCampus">Campus</label> <input type="text"
							class="form-control" id="txtCampus"
							placeholder="Nombre del Campus">
					</div>
					
					
					
					
					<button type="submit" class="btn btn-info">
						<span class="glyphicon glyphicon-ok"></span> Crear
					</button>
					<button type="submit" class="btn btn-info">
						<span class="glyphicon glyphicon-edit"></span> Modificar
					</button>
					<button type="submit" class="btn btn-info">
						<span class="glyphicon glyphicon-trash"></span> Eliminar
					</button>
				</form>
			</div>
		</div>
	</div>
</div>